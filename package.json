    "@tiptap/extension-task-item": "2.0.0-beta.217",
    "@tiptap/extension-task-list": "2.0.0-beta.217",
    "@tiptap/extension-youtube": "2.0.0-beta.217",
    "@tiptap/starter-kit": "2.0.0-beta.217",
    "cookie": "^0.5.0",
    "daisyui": "^2.50.0",
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "firebase": "^9.19.1",
    "idb": "^7.1.1",
    "theme-change": "^2.3.0"
  }
}
