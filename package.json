{
  "name": "top-sveltekit",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "npm run build && electron-builder --config electron-builder.config.json",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "electron": "concurrently --kill-others \"vite dev\" \"electron src/electron.cjs\"",
    "dev:package": "npm run build && electron-builder --config electron-builder.config.json --dir"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.2.1",
    "@popperjs/core": "^2.11.6",
    "@sveltejs/adapter-vercel": "^2.0.2",
    "@sveltejs/kit": "^1.5.6",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "concurrently": "^7.6.0",
    "electron-builder": "^23.6.0",
    "electron-reloader": "^1.2.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "mdsvex": "^0.10.6",
    "nprogress": "^0.2.0",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-heros": "^2.6.0",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.6",
    "tslib": "^2.5.0",
    "typescript": "~4.9.5",
    "vite": "^4.1.1"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@lukeed/uuid": "^2.0.0",
    "@poppanator/sveltekit-svg": "^2.1.2",
    "@supabase/supabase-js": "^2.7.1",
    "@tiptap/core": "2.0.0-beta.217",
    "@tiptap/extension-link": "2.0.0-beta.217",
